.container {
  padding: 15rpx 50rpx;
}
input{
  color: #fff;
}
.article-title input{
  height: 74rpx;
}
.article-content{
  padding: 25rpx 0;
  color: #fff;
  line-height: 40rpx;
}

.photo{
  padding: 43rpx 0 23rpx;
  overflow: hidden;
}
.img-item{
  position: relative;
  float: left;
  height: 115rpx;
  width: 115rpx;
  margin: 0 25rpx 20rpx 0;
  border-radius: 15rpx;
  border: 1px solid #9900ff;
  overflow: hidden;
}
.image-delete {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  height: 42rpx;
  width: 42rpx;
  line-height: 42rpx;
  background: rgba(0, 0, 0, 0.64);
  border-radius: 50%;
  color: #fff;
  font-weight: bolder;
  font-size: 22rpx;
  z-index: 10;
  text-align: center;
}
.img-item .img{
  width: 100%;
}
.addImg{
  margin: 30rpx;
  height: 55rpx!important;
  width: 55rpx!important;
}
.topic{
  position: relative;
  height: 120rpx;
  line-height: 120rpx;
}
.topic-icon{
  height: 42rpx;
  width: 34rpx;
  margin-right: 19rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
.address-icon{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  height: 42rpx;
  width: 34rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
.address{
  padding-left: 53rpx;
}
.release-btn{
  height: 140rpx;
  width: 140rpx;
  margin: 50rpx auto;
  border-radius: 100%;
  border: 10rpx solid #ff00ff;
  text-align: center;
  font-size: 43rpx;
  color: #ff00ff;
  line-height: 140rpx;
  
}
