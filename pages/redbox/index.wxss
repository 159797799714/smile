.container,
page {
  background: #131313;
}

/* 顶部导航 */
.tab{
  overflow: auto;
  flex-wrap: nowrap;
}
.tab .tabbar{
  min-width: 140rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.selected{
  position: relative;
  font-size: 34rpx;
  color: #fff;
}
.selected:after{
  content: '';
  display: block;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  height: 4rpx;
  width: 34rpx;
  background: #ff00ff;
}
.remarks{
  padding: 36rpx 60rpx 0;
  line-height: 26rpx;
}
/* 优惠券列表 */
.coupon-list {
  padding: 20rpx 60rpx;
}

.coupon-list .coupon-item {
  margin-top: 25rpx;
  box-sizing: border-box;
  height: 143rpx;
  width: 100%;
  border-radius: 30rpx;
}
.b-linear{
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}
.b-cc{
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}
.coupon-item .coupon-left{
  width: 300rpx;
  height: 100%;
  padding-left: 20rpx;
  overflow: hidden;
}
.coupon-item .name{
  width: 160rpx;
  margin: auto 0;
  line-height: 50rpx;
  text-shadow:0px 3px 7px rgba(0, 0, 0, 0.13);
}
.col-FD{
  color: #FDAB3B;
}

.coupon-item .num{
  width: 128rpx;
  height: 107rpx;
  margin: auto 0;
  padding-left: 10rpx;
  line-height: 107rpx;
}
.coupon-item .coupon-left .discount{
  position: relative;
  min-width: 130rpx;
  line-height: 78rpx;
  white-space: nowrap;
}
.coupon-item .discount .word{
  line-height: 32rpx;
  height: 32rpx;
  margin-top: 40rpx;
}
.coupon-item .coupon-info{
  width: 166rpx;
  text-align: center;
  line-height: 28rpx;
  overflow: hidden;
}
.word-space-1{
  letter-spacing: 2rpx;
}
.coupon-item .onelist-hidden{
  text-overflow: initial;
}

.coupon-item .coupon-right{
  width: 70rpx;
  margin-right: 40rpx;
  text-align: center;
}
.coupon-item .coupon-right> text{
  display: block;
  padding: 15rpx;
  margin: auto;
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  line-height: 20rpx;
}
