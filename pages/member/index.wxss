.main{
  padding-top: 50rpx;
}
.user-avatar{
  margin: 0 auto;
  height: 80rpx;
  width: 80rpx;
  border-radius: 100%;
  border: 1px solid #666;
  overflow: hidden;
}
.name{
  margin-bottom: 3rpx;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.item-title{
  padding: 0 60rpx;
  line-height: 130rpx;
}
.grade-img{
  margin: 0 auto;
  width: 500rpx;
  height: 90rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
}
.more{
  padding: 0 110rpx;
}
.more .item{
  float: left;
  width: 25%;
}
.item .item-img{
  margin: 0 auto;
  margin-bottom: 25rpx;
  height: 60rpx;
  width: 57rpx;
  overflow: hidden;
}
.item-img>image{
  width: 100%;
}
.item .word{
  line-height: 74rpx;
}