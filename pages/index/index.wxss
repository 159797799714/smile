/* diy组件 */
@import "../../components/diy/index.wxss";

page {
  background: #131313 !important;
  padding-top: 50px;
  box-sizing: border-box;
}

.header-bar {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 64px;
  padding-top: 24px;
  box-sizing: border-box;
  background-color: #131313;
  z-index: 9;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

.header-bar .name {
  font-size: 38rpx;
  margin-right: 5px;
  font-weight: 700;
}

.header-bar .logo {
  background-image: url(data:image/png;base64,);
  width: 100px;
  height: 100px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

/* 搜索框 */
.index-search-box-bg {
    background-color: #131313;
    height: 130rpx;
    padding-top: 20rpx;
    margin-bottom: 20rpx;
}
.index-search-box {
    height: 72rpx;
    margin: 10rpx 30rpx 20rpx;
}
.index-cont-search {
    margin-top: 26rpx;
    height: 72rpx;
    background-color: rgba(248,248,248,.1);
    border-radius:4rpx;
}
.index-cont-search>image {
    width: 28rpx;
}
.index-cont-search text {
    margin-left: 22rpx;
}