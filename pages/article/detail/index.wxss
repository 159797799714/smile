/* wxParse.wxss */
@import "/wxParse/wxParse.wxss";
/* diy组件 */
@import "../../../components/diy/index.wxss";

page {
  background: #000;
}

.container {
  padding: 30rpx;
}

.wxParse-img {
  width: 706rpx;
}

.article-little-image {
  width: 750rpx;
  /* margin-top: 20rpx; */
}

.goods {
    display: flex;
    height: 290rpx;
    width: 100%;
    padding: 20rpx;
    border-top: 1px solid #ededed;
    border-bottom: 1px solid #ededed;
    box-sizing: border-box;
}

.left {
  width: 250rpx;
  margin-right: 30rpx;
}

.right {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-bottom: 20rpx;
  position: relative;
}

.btn-to-goods {
  width: 180rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 4rpx;
  text-align: center;
  position: absolute;
  right: 20rpx;
  bottom: 10rpx;
  background-color: #FF5C00;
  color: #fff;
}

.goodsImg {
  width: 100%;
  height: 100%;
}

.like-wrap {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 40rpx 0 100rpx 0;
  flex-direction: column;
  color: #999;
  font-size: 26rpx;
}

/* 评论 */
.comment-wrap {
    padding-top: 40rpx;
    background-color: #F5F5F5;
}
.comment-wrap > view {
    padding-bottom: 98rpx;
}
.comment-wrap-content {
    padding: 40rpx 40rpx 0;
}
.comment-wrap-content-people {
    padding-bottom: 50rpx;
}
.uesr-photo image {
    width:80rpx;
    height:80rpx;
    border-radius:50%;
    margin-right: 35rpx;
}
.like-number image {
    width: 26rpx;
}
.see-all-comment {
    padding-left: 150rpx;
    padding-bottom: 60rpx;
}

/* 没有评论 */
.no-comment {
    text-align: center;
}
.no-comment> image {
    width: 112rpx;
    margin: 35rpx 0 24rpx;
}
.no-comment> view {
    margin-bottom: 70rpx;
}

/* 查看所有评论 */
.all-comment {
    height: 97vh;
    padding: 50rpx 40rpx 90rpx;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 11111;
    border-radius: 12rpx 12rpx 0 0;
    transition: all .4s;
}

.all-comment.close-style {
    height: 0;
    padding: 0;
    transition: all .4s;
}
.post-comment.close-style {
    padding: 0;
    height: 0;
    transition: all .01s;
}
.all-comment-text>image {
    width: 27rpx;
}
.all-comment .all-comment-content {
    margin-top: 50rpx;
}
.all-comment .all-comment-content:last-child {
    margin-bottom: 97rpx;
}
/* 作者回复 */
.authors-reply {
    width:555rpx;
    background:rgba(245,245,245,1);
    border-radius:4rpx;
    padding: 36rpx 20rpx;
    margin: -20rpx 0 50rpx 114rpx;
    box-sizing: border-box;
    -webkit-overflow-scrolling: touch;
}
.authors-reply> .authors-reply-line {
    width:4rpx;
    height:27rpx;
    background:rgba(240,90,37,1);
    border-radius:2rpx;
}
.authors-reply> .authors-reply-content {
    margin-left: 14rpx;
}
.authors-reply> .authors-reply-content>text {
    display: block;
    line-height: 28rpx;
}
.authors-reply>.authors-reply-like image {
    width: 26rpx;
    display: block;
}
/* 发布评论 */
.post-comment {
    position: fixed;
    bottom: 0;
    right: 0;
    width: 100%;
    z-index: 11111111111111112;
    overflow-y: scroll;
    padding: 26rpx 39rpx;
    box-sizing: border-box;
    overflow: hidden;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    transition: all .01s;
}
.post-comment> view {
    position: relative;
    z-index: 11111111111111113;
}
.post-comment textarea {
    width: 560rpx;
}

/* 遮罩层 */
.mask {
    height: 100vh;
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 11111;
    background-color: rgba(0, 0, 0, 0.3);
}

/* 文章状态 */
.article-status {
    height: 98rpx;
    position: fixed;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    border-top:1rpx solid rgba(221,221,221,1);
}
.article-status> view {
    width: 33.33%;
    height: 100%;
    align-items: center;
}
.article-status image {
    margin-right: 18rpx;
}
.article-status .article-status-see image {
    width: 52rpx;
    margin-left: 57rpx;
}
.article-status .article-status-like image {
    width: 36rpx;
    margin-left: 80rpx;
}
.article-status .article-status-comment image {
    width: 40rpx;
}

.like {
  background-color: #FF5C00;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.active {
  opacity: 0.6;
}

.icon-like {
  width: 38rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-position: center;
  background-repeat: no-repeat;
}

/* .diy-component-my .diy-banner {
    margin: 0 !important;
} */
.my-class111 .diy-banner {
    margin: 0;
}