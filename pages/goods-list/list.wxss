/* 商品样式1 */
.main{
  padding: 0 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  overflow: hidden;
}
.main .item{
  height: 524rpx;
  width: 330rpx;
  margin-bottom: 20rpx;
  border: 1px solid #eee;
}
.main .item .img{
  height: 330rpx;
  width: 100%;
  margin-bottom: 18rpx;
}
.main .item .img>image{
  height: 100%;
  width: 100%;
  background: #ccc;
}
.main .item .title{
  height: 67rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  margin-bottom: 24rpx;
  padding: 0 20rpx;
}
.main .item .info{
  display: flex;
  justify-content: space-between;
  height: 40rpx;
  width: 100%;
  padding: 0 20rpx;
  line-height: 40rpx;
}
.main .item .info .zan{
  display: flex;
  color: #ADADAD;
  font-size: 24rpx;
}
.main .item .info .zan image {
  width: 29rpx;
  margin-right: 8rpx;
  margin-top: 6rpx;
}
.main .item .info .user{
  display: flex;
}
.main .item .info .user .userName{
  font-size: 24rpx;
  font-weight: bold;
  width: 150rpx;
}
.main .item .info .user .userImg{ 
  height: 40rpx;
  width: 40rpx;
  margin-right: 10rpx;
  border-radius: 100%;
  overflow: hidden;
}
.main .item .info .user .userImg>image{
  height: 100%;
  width: 100%;
  background: #ccc;
}
.main .good-item{
  display: flex;
  flex-direction: column;
  height: 550rpx;
  width: 330rpx;
  margin-top: 20rpx;
  border: 1px solid #eee;
}
.main .good-item .good-img{
  height: 330rpx;
  width: 330rpx;
  margin-bottom: 30rpx;
  background: #f5f5f5;
}
.good-item .good-img {
  overflow: hidden;
}
.good-item .good-img image {
  width: 100%;
}
.main .good-item .good-info{
  flex: 1;
   
  
  padding: 7rpx 18rpx;
}
.main .good-item .good-info .good-name{
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
}
.main .good-item .good-info .good-remark{
  font-size: 20rpx;
  line-height: 20px;
  color: #666;
}
.main .good-item .good-info .good-remark>text{
  margin-right: 32rpx;
}
.main .good-item .good-info .max-price{
  line-height: 26rpx;
}
.main .good-item .good-info .good-price{
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  font-weight: 600;
  line-height: 54rpx;
}
.main .good-item .good-info .good-price .bigText{
  font-size: 36;
}
/* 样式2 */
.row{
  padding: 0 30rpx;
}
.row .good-item{
  height: 220rpx;
  width: 100%;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  display: flex;
}
.row .good-item .good-img{
  height: 220rpx;
  width: 220rpx;
  margin-right: 30rpx;
  background: #ccc;
}
.row .good-item .good-info{
  flex: 1;
   
  
  overflow: hidden;
}
.row .good-item .good-info .good-price{
  line-height: 54rpx;
}
.good-item .good-info .good-price image {
  width: 26rpx;
}
.row .good-item .good-info .good-price .bigText{
  font-size: 34rpx;
}
.row .good-item .good-info .good-name{
  height: 78rpx;
  line-height: 42rpx;
  margin-top: 14rpx;
  font-size: 28rpx;
}
.row .good-item .good-info .good-remark{
  font-size: 20rpx;
  line-height: 59rpx;
  color: #666;
}
.row .good-item .good-info .good-remark>text{
  margin-right: 29rpx;
}

/* 筛选 */
.big-cover{
  flex-direction: row;
  position:fixed;
  height:100%;
  width:100%;
  top:0;
  right:0;
  display: flex;
  z-index: 6;
  background:rgba(0, 0, 0, 0.6);
}
.big-cover .white{
  flex: 1;
  
}
.big-cover .big-cover-main{
  width: 524rpx;
  display: flex;
  flex-direction: column;
  padding: 0 30rpx;
  overflow:hidden;white-space:nowrap;
}
.big-cover .big-cover-main .box{
  flex: 1;
  overflow: hidden;
  height: 1118rpx;
}
.big-cover .big-cover-main .title{
  font-size: 26rpx;
  line-height: 85rpx;
}
.big-cover .big-cover-main .price{
  height: 159rpx;
  margin: 0rpx 5rpx;
  padding-bottom: 30rpx;
}
.big-cover .big-cover-main .price .ipt{
  height: 74rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.big-cover .big-cover-main .price .ipt::after{
  content: '';
  position: absolute;
  height: 2rpx;
  width: 30rpx;
  top: 50%;
  left: 50%;
  right: 0;
  bottom: 0;
  transform: translate(-50%);
  background: #eee;
}
.big-cover .big-cover-main .price .ipt>input{
  width: 190rpx;
  height: 74rpx;
  font-size: 26rpx;
  text-align: center;
  border-radius: 37rpx;
  background: #eee;
}
.big-cover .big-cover-main .caption{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 2px solid #f5f5f5;
}
.big-cover .big-cover-main .caption image {
  width: 21rpx;
}
.big-cover .big-cover-main .tag-span{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  min-height: 10rpx;
  font-size: 26rpx;
}
.big-cover .big-cover-main .tag-span .tag{
  width: 216rpx;
  height: 88rpx;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
  line-height: 88rpx;
  text-align: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  background: #eee;
}
.big-cover .big-cover-main .foot{
  width: calc(100% + 60rpx);
  margin-left: -30rpx;
}


.list-header {
  position: fixed;
  top: -1rpx;
  left: 0;
  right: 0;
}

.no-more {
  text-align: center;
  color: #737373;
  padding: 20rpx 0;
}

.category-list {
  overflow: hidden;
}

.category-list .list {
  box-sizing: border-box;
  width: 50%;
  float: left;
}

.category-list .list:nth-child(2n) {
  border-left: 4rpx solid #f7f7f7;
  border-bottom: 8rpx solid #f7f7f7;
}

.category-list .list:nth-child(2n-1) {
  border-right: 4rpx solid #f7f7f7;
  border-bottom: 8rpx solid #f7f7f7;
}

.category-list .list .left, .category-list .right {
  width: 100%;
}

.category-list .list .left .img image {
  width: 100%;
  height: 375rpx;
  display: block;
}

.category-list .right .cont {
  padding: 0 12rpx;
}

.category-list .right .cont .title {
  height: 76rpx;
  line-height: 1.3;
}

.category-list.arrange .list {
  overflow: hidden;
  padding: 15rpx;
  border-bottom: 1rpx solid #f7f7f7;
  width: 100%;
}

.category-list.arrange .list .left {
  width: 35%;
  float: left;
}

.category-list.arrange .list .right {
  width: 65%;
  float: left;
}

.category-list.arrange .list .left .img image {
  width: 220rpx;
  height: 220rpx;
}

.reset {
  box-shadow:0px 6px 18px 2px rgba(0, 0, 0, 0.3);
}