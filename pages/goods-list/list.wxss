page {
  background: #fff;
}

.store_nav_cont {
  padding: 20rpx 0;
}

.store_nav_cont #list-true {
  color: #16171c;
  font-size: 28rpx;
  font-weight:800;
}

.store_nav_cont #list-0 {
  color: #333;
  font-size: 28rpx;
}

.store_nav_cont .price__arrow {
  padding-left: 20rpx;
  font-size: 18rpx;
  color: #777;
}

.store_nav_cont #list-true .price__arrow text.active {
  color: #16171c;
}

.container {
  position: relative;
}

.index-cont-search {
  background:rgba(248,248,248,.2);
  height: 72rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.index-cont-search image {
  width: 28rpx;
}

.index-search-box {
  background: #131313;
  padding: 18rpx 13rpx 0;
}

.index-search {
  border-bottom: 0;
  border-radius: 4rpx;
  overflow: hidden;
  position: relative;
  font-size: 28rpx;
  color: #fff;
  box-sizing: border-box;
  height: 72rpx;
  line-height: 72rpx;
  margin: 0 43rpx;
}

.index-cont-search input {
    margin-left: 34rpx;
    line-height: 72rpx;
    flex: 1;
     
    
    /* margin-right: 280rpx; */
}

.list-right {
  width: 60rpx;
}
.list-right image {
    width: 32rpx;
    margin-top: 14rpx;
}

/* 分享或者商城 */
.tabs{
  display: flex;
  justify-content: space-around;
  height: 122rpx;
  padding: 0 150rpx;
  font-size: 34rpx;
  line-height: 122rpx;
  font-weight: 500;
  color: #ADADAD;
  background: #131313;
}
.selectTab{
  color: #fff;
  position: relative;
}
.selectTab::after{
  content: '';
  height: 4rpx;
  width: 34rpx;
  background: #F4433D;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}

/* 综合等分类 */
.filter{
  display: flex;
  /* justify-content: space-between; */
  justify-content: space-around;
  height: 92rpx;
  padding: 0 20rpx;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  color: #666;
  line-height: 92rpx;
}
.filter>view {

}
.filter .iconfont{
  margin-left: 14rpx;
}
.selectFilter{
  color: #F4433D;
  font-weight: 600;
}
.selectFilter .iconfont{
  font-weight: 400;
}
.filter .price-icon {
  width: 12rpx;
}
.filter .filter-icon {
  width: 23rpx;
  margin-left: 6rpx;
}

/* 品牌及分类 */
.filter-box {
  position: relative;
}
.filter-tags{
  height: 50rpx;
  padding: 0 30rpx 30rpx;
  display: flex;
}
.filter-tags .filter-tag{
  width: 120rpx;
  margin-right: 22rpx;
  text-align: center;
  font-size: 22rpx;
  line-height: 50rpx;
  background: #eee;
  border-radius: 25rpx;
}
.filter-tags image{
  margin-left: 6rpx;
  width: 14rpx;
  height: 6rpx;
  vertical-align: middle;
}
.filter-cover{
  display: flex;
  flex-direction: column;
  height: 100vh;
  width: 100%;
  position: absolute;
  top: 180rpx;
  background: rgba(0, 0, 0, 0.6);
  z-index: 2;
}
.filter-cover .cover-main{
  display: flex;
  flex-direction: column;
  width: 100%;
  /* min-height: 236rpx;
  max-height: 800rpx; */
}
.filter-cover .cover-main .list{
  flex: 1;
  /* min-height: 70rpx; */
  height: 324rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.filter-cover .cover-main .list>view{
  height: 88rpx;
  width: 216rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  line-height: 88rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  background: #eee;
}
.filter-cover .cover-main .list>view:nth-child(3n) {
  margin-right: 0;
}
.filter-cover .white{
  flex: 1;
   
  
}
.foot{
  height: 98rpx;
  display: flex;
  font-size: 30rpx;
  color: #666;
  text-align: center;
  line-height: 98rpx;
  border-top: 1px solid f5f5f5;
}
.foot>view{
  flex: 1;
   
  
}
.foot .sure{
  color: #fff;
  background: #131313;
}

/* 分享或商品列表 */
.culture{
  /* flex: 1;
  display: flex;
  flex-direction: column; */
