.good-video{
  position: relative;
  height: 100%;
  width: 100%;
}
.video-bg{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 100;
}
.play-icon{
  margin: 0 auto;
  margin-top: 50%;
  transform: translateY(-50%);
  height: 120rpx;
  width: 120rpx;
  border-radius: 100%;
  background: url(data:image/jpeg;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}