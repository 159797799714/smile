/* 无样式button (用于伪submit) */

.btn-normal {
  display: block;
  margin: 0;
  padding: 0;
  line-height: normal;
  background: none;
  border-radius: 0;
  box-shadow: none;
  border: none;
  font-size: unset;
  text-align: unset;
  overflow: visible;
}

.btn-normal:after {
  border: none;
}

.btn-normal.button-hover {
  color: inherit;
}

.my-button{
  border-radius: 25rpx;
  text-align: center;
}

.selectSpan{
  position: relative;
  color: #3333ff;
}
.selectSpan:before{
  content: '+';
  position: absolute;
  bottom: -20rpx;
  right: -20rpx;
  color: #fff;
  font-size: 22rpx;
  height: 42rpx;
  width: 42rpx;
  text-align: left;
  line-height: 40rpx;
  background: #3333ff;
  transform: rotate(45deg);
  z-index: 5;
}

.none{
  background: #666666;
}
.states{
  position: absolute;
  height: 150rpx;
  width: 150rpx;
  overflow: hidden;
}
.pingtuan-status{
  position: relative;
  height: 140rpx;
  width: 140rpx;
  text-align: center;
  border-radius: 100%;
  border: 25rpx double #ff33cc;
  color: #ff33cc;
}
.pingtuan-status>text{
  position: absolute;
  left: -3rpx;
  top: -3rpx;
  height: 146rpx;
  width: 146rpx;
  box-sizing: border-box;
  /* padding: 30rpx 25rpx 0; */
  /* padding: 50rpx 5rpx 0; */
  border-radius: 100%;
  background: #131313;
  transform: rotate(-35deg);
  font-size: 40rpx;
  font-weight: bold;
  line-height: 40rpx;
}
.style-66{
  border-color: #666!important;
  color: #666!important;
}
.num-btn{
  line-height: 45rpx;
  width: 50rpx;
  text-align: center;
}

/* 拼团状态 */
.order-status{
  position: absolute;
  top: 140rpx;
  right: 50rpx;
  height: 150rpx;
  width: 150rpx;
  border-radius: 100%;
  font-size: 35rpx;
  font-weight: bold;
  color: #ff00ff;
  border: 5rpx solid #ff00ff;
  opacity: 0.6;
}


.order-status .pingtuan-order-status{
  margin: 15rpx 0 0 15rpx;
  box-sizing: border-box;
  height: 120rpx;
  width: 120rpx;
  border-radius: 100%;
  border: 3rpx solid #ff00ff;
  transform: rotate(-35deg);
}
